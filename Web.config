<configuration>
  <system.webServer>
      <staticContent>
        <mimeMap fileExtension=".json" mimeType="application/json" />
        <mimeMap fileExtension=".woff" mimeType="font/x-woff" />
        <mimeMap fileExtension=".woff2" mimeType="font/woff2" />
      </staticContent>
      <defaultDocument>
        <files>
          <clear />
          <add value="index.html" />
        </files>
      </defaultDocument>
   </system.webServer>
</configuration>
